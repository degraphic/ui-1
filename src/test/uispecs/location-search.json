    ".csc-location-type": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "locationTypes",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-location-type": {
                                            "optionnames": [
                                                "Please select a value",
                                                "Shelf",
                                                "Tray",
                                                "Drawer",
                                                "Unit",
                                                "Room",
                                                "Building"
                                            ],
                                            "optionlist": [
                                                "",
                                                "shelf",
                                                "tray",
                                                "drawer",
                                                "unit",
                                                "room",
                                                "building"
                                            ],
                                            "selection": "${{row}.locationType}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "locationTypes"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },
