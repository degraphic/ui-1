    },
    ".csc-location-editDate-label": {
        "messagekey": "location-editDate"
    },
    ".csc-location-editDateStart-label": {
        "messagekey": "location-editDateStartLabel"
    },
    ".csc-location-editDateStart": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${editDateStart}"
    },
    ".csc-location-editDateEnd-label": {
        "messagekey": "location-editDateEndLabel"
    },
    ".csc-location-editDateEnd": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${editDateEnd}"
    },
    ".csc-location-editUsername-label": {
        "messagekey": "location-editUsername"
    },
    ".csc-location-editUsername": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "editUsernames",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-location-editUsername": {
                                            "decorators": [
                                                {
                                                    "func": "cspace.autocomplete",
                                                    "type": "fluid",
                                                    "options": {
                                                        "queryUrl": "../../../tenant/core/location/autocomplete/editUsername",
                                                        "vocabUrl": "../../../tenant/core/location/source-vocab/editUsername"
                                                    }
                                                }
                                            ],
                                            "value": "${{row}.editUsername}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "editUsernames"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    }
}