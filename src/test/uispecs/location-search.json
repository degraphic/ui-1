{
    ".csc-location-accessNote-label": {
        "messagekey": "location-accessNoteLabel"
    },
    ".csc-location-accessNote": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "accessNotes",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-location-accessNote": "${{row}.accessNote}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "accessNotes"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "hidePrimary": true
                }
            }
        ]
    },
    ".csc-location-address-label": {
        "messagekey": "location-addressLabel"
    },
    ".csc-location-type": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "locationTypes",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-location-type": {
                                            "optionnames": [
                                                "Please select a value",
                                                "Tray",
                                                "Shelf",
                                                "Drawer",
                                                "Building",
                                                "Unit",
                                                "Room"
                                            ],
                                            "optionlist": [
                                                "",
                                                "tray",
                                                "shelf",
                                                "drawer",
                                                "building",
                                                "unit",
                                                "room"
                                            ],
                                            "selection": "${{row}.locationType}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "locationTypes"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "hidePrimary": true
                }
            }
        ]
    },
    ".csc-location-updatedAtStart": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${updatedAtStart}"
    },
    ".csc-location-updatedAtEnd": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
