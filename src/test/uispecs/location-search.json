                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-location-termStatus": {
                                            "optionnames": [
                                                "Provisional",
                                                "Under Review",
                                                "Accepted",
                                                "Rejected"
                                            ],
                                            "optionlist": [
                                                "provisional",
                                                "under review",
                                                "accepted",
                                                "rejected"
                                            ],
                                            "selection": "${{row}.termStatus}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
