{
    ".csc-organization-foundingDate-label": {
        "messagekey": "organization-foundingDateLabel"
    },
    ".csc-organization-foundingDateStart-label": {
        "messagekey": "organization-foundingDateStartLabel"
    },
    ".csc-organizationAuthority-displayName": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "displayNames",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-organizationAuthority-displayName": "${{row}.displayName}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "displayNames"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "hidePrimary": true
                }
            }
        ]
    },
    ".csc-organization-group-label": {
        "messagekey": "organization-groupLabel"
    },
    ".csc-organization-dissolutionDateEnd-label": {
        "messagekey": "organization-dissolutionDateEndLabel"
    },
    ".csc-organization-dissolutionDateStart": {
        "decorators": [
            {
