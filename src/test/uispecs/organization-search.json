    ".csc-organization-termStatus-label": {
        "messagekey": "organization-termStatusLabel"
    },
    ".csc-organization-termStatus": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "termStatuses",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-organization-termStatus": {
                                            "default": "provisional",
                                            "optionnames": [
                                                "Provisional",
                                                "Under Review",
                                                "Accepted",
                                                "Rejected"
                                            ],
                                            "optionlist": [
                                                "provisional",
                                                "under review",
                                                "accepted",
                                                "rejected"
                                            ],
                                            "selection": "${{row}.termStatus}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "termStatuses"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },
