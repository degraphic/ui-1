    ".csc-organization-editUsername-label": {
        "messagekey": "organization-editUsername"
    },
    ".csc-organization-editUsername": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "editUsernames",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-organization-editUsername": {
                                            "decorators": [
                                                {
                                                    "func": "cspace.autocomplete",
                                                    "type": "fluid",
                                                    "options": {
                                                        "queryUrl": "../../../tenant/core/organization/autocomplete/editUsername",
                                                        "vocabUrl": "../../../tenant/core/organization/source-vocab/editUsername"
                                                    }
                                                }
                                            ],
                                            "value": "${{row}.editUsername}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "editUsernames"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    }
