{
    ".csc-organization-displayName-label": {
        "messagekey": "organization-displayName-label"
    },
    ".csc-organization-displayName": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "displayNames",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-organization-displayName": "${{row}.displayName}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "displayNames"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },
    ".csc-organization-foundingPlace-label": {
        "messagekey": "organization-foundingPlaceLabel"
