{
    ".csc-person-displayName-label": {
        "messagekey": "person-displayNameLabel"
    },
    ".csc-person-displayName": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "displayNames",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-person-displayName": "${{row}.displayName}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "displayNames"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },
