    ".csc-person-birthDateEnd": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${birthDateEnd}"
    },
    ".csc-personAuthority-status": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "termStatuss",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-personAuthority-status": {
                                            "default": "provisional",
                                            "optionnames": [
                                                "Provisional",
                                                "Under Review",
                                                "Accepted",
                                                "Rejected"
                                            ],
                                            "optionlist": [
                                                "provisional",
                                                "under review",
                                                "accepted",
                                                "rejected"
                                            ],
                                            "selection": "${{row}.termStatus}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "termStatuss"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "hidePrimary": true
                }
            }
        ]
    },
    ".csc-person-deathDateEnd": {
        "decorators": [
