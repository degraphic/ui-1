                            "expander": {
                                "tree": {
                                    "expander": {
                                        "repeatID": "repeat:",
                                        "tree": {
                                            ".csc-media-types": {
                                                "default": "0",
                                                "optionnames": [
                                                    "Please select a value",
                                                    "Dataset",
                                                    "Document",
                                                    "Moving Image",
