                                                    "German",
                                                    "Swahili",
                                                    "Italian",
                                                    "Middle English",
                                                    "Latin",
                                                    "Japanese",
                                                    "Spanish" 
                                                ],
                                                "optionlist": [
                                                    "",
                                                    "oldenglish",
                                                    "chinese",
                                                    "eng",
                                                    "hebrew",
                                                    "ancientgreek",
                                                    "portuguese",
                                                    "arabic",
                                                    "russian",
                                                    "korean",
                                                    "french",
                                                    "german",
                                                    "swahili",
                                                    "italian",
                                                    "middleenglish",
                                                    "latin",
                                                    "japanese",
                                                    "spn" 
                                                ],
                                                "selection": "${{row}.language}" 
                                            } 
                                        },
                                        "type": "fluid.renderer.repeat",
                                        "pathAs": "row",
                                        "controlledBy": "fields.languages" 
                                    } 
                                },
                                "type": "fluid.noexpand" 
                            } 
                        } 
                    } 
                } 
            ] 
        },
        ".csc-media-location": "${fields.location}",
        ".csc-media-publisher": {
            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/media/autocomplete/publisher",
                        "vocabUrl": "../../chain/media/source-vocab/publisher" 
                    } 
                } 
            ],
            "value": "${fields.publisher}" 
        },
        ".csc-media-relations": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.relations",
                        "protoTree": {
                            "expander": {
                                "tree": {
                                    "expander": {
                                        "repeatID": "repeat:",
                                        "tree": {
                                            ".csc-media-relations": "${{row}.relation}" 
                                        },
                                        "type": "fluid.renderer.repeat",
                                        "pathAs": "row",
                                        "controlledBy": "fields.relations" 
                                    } 
                                },
                                "type": "fluid.noexpand" 
                            } 
                        } 
                    } 
                } 
            ] 
        },
        ".csc-media-rightsHolder": {
            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/media/autocomplete/rightsHolder",
                        "vocabUrl": "../../chain/media/source-vocab/rightsHolder" 
                    } 
                } 
            ],
            "value": "${fields.rightsHolder}" 
        },
        ".csc-media-source": "${fields.source}",
        ".csc-media-subjects": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.subjects",
                        "protoTree": {
                            "expander": {
                                "tree": {
                                    "expander": {
                                        "repeatID": "repeat:",
                                        "tree": {
                                            ".csc-media-subjects": "${{row}.subject}" 
                                        },
                                        "type": "fluid.renderer.repeat",
                                        "pathAs": "row",
                                        "controlledBy": "fields.subjects" 
                                    } 
