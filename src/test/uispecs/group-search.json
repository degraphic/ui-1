{
    ".csc-group-title-label": {
        "messagekey": "group-titleLabel"
    },
    ".csc-group-owner": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "owners",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-group-owner": {
                                            "decorators": [
                                                {
                                                    "func": "cspace.autocomplete",
                                                    "type": "fluid",
                                                    "options": {
                                                        "queryUrl": "../../chain/group/autocomplete/owner",
                                                        "vocabUrl": "../../chain/group/source-vocab/owner"
                                                    }
                                                }
                                            ],
                                            "value": "${{row}.owner}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "owners"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },
    ".csc-group-title": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "titles",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-group-title": "${{row}.title}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "titles"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },
    ".csc-group-owner-label": {
        "messagekey": "group-ownerLabel"
    },
    ".csc-group-editDate-label": {
        "messagekey": "group-editDate"
    },
    ".csc-group-editDateStart-label": {
        "messagekey": "group-editDateStartLabel"
    },
    ".csc-group-editDateStart": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${editDateStart}"
    },
    ".csc-group-editDateEnd-label": {
        "messagekey": "group-editDateEndLabel"
    },
    ".csc-group-editDateEnd": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${editDateEnd}"
    },
    ".csc-group-editUsername-label": {
        "messagekey": "group-editUsername"
    },
    ".csc-group-editUsername": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "editUsernames",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-group-editUsername": {
                                            "decorators": [
                                                {
                                                    "func": "cspace.autocomplete",
                                                    "type": "fluid",
                                                    "options": {
                                                        "queryUrl": "../../../tenant/core/group/autocomplete/editUsername",
                                                        "vocabUrl": "../../../tenant/core/group/source-vocab/editUsername"
                                                    }
                                                }
                                            ],
                                            "value": "${{row}.editUsername}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "editUsernames"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    }
}