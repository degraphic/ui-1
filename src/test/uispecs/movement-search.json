    ".csc-movement-movementContact": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "movementContacts",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-movement-movementContact": {
                                            "decorators": [
                                                {
                                                    "func": "cspace.autocomplete",
                                                    "type": "fluid",
                                                    "options": {
                                                        "queryUrl": "../../../tenant/core/movement/autocomplete/movementContact",
                                                        "vocabUrl": "../../../tenant/core/movement/source-vocab/movementContact"
                                                    }
                                                }
                                            ],
                                            "value": "${{row}.movementContact}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "movementContacts"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "hidePrimary": true
                }
            }
        ]
    },
    ".csc-movement-reasonForMove-label": {
        "messagekey": "movement-reasonForMoveLabel"
    },
