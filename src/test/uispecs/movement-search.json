{
    ".csc-movement-locationDateEnd": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${locationDateEnd}"
    },
    ".csc-movement-movementReferenceNumber-label": {
        "messagekey": "movement-movementReferenceNumberLabel"
    },
    ".csc-movement-updatedAtStart-label": {
