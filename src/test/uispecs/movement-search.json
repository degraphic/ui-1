{
    ".csc-movement-currentLocation-label": {
        "messagekey": "movement-currentLocationLabel"
    },
    ".csc-movement-currentLocation": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "hidePrimary": true,
                    "elPath": "currentLocations",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-movement-currentLocation": {
                                            "decorators": [
                                                {
                                                    "func": "cspace.autocomplete",
                                                    "type": "fluid",
                                                    "options": {
                                                        "queryUrl": "../../../tenant/core/movement/autocomplete/currentLocation",
                                                        "vocabUrl": "../../../tenant/core/movement/source-vocab/currentLocation"
                                                    }
                                                }
                                            ],
                                            "value": "${{row}.currentLocation}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "currentLocations"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }
        ]
    },
