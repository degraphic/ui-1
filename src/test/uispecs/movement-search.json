                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-movement-movementMethods": {
                                            "default": "",
                                            "optionnames": [
                                                "Please select a value",
                                                "Forklift",
                                                "Handcarried",
                                                "Trolley"
                                            ],
                                            "optionlist": [
                                                "",
                                                "forklift",
                                                "handcarried",
                                                "trolley"
                                            ],
                                            "selection": "${{row}.movementMethod}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "movementMethods"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "hidePrimary": true
                }
            }
        ]
    },
