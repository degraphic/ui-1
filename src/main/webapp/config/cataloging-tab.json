{
    "schema": [
        "recordlist",
        "recordtypes",
        "cataloging"
    ],
    "pageType": "cataloging-tab",
    "recordType": "cataloging",
    "pageSpec": {
        "list": {
            "href": "CatalogingTabRecordListTemplate.html",
            "templateSelector": ".csc-cataloging-tab-record-list",
            "targetSelector": ".csc-cataloging-tab .div-for-list-of-records" 
        },
        "details": {
            "href": "%readonlypages/CatalogingTemplate.html",
            "templateSelector": ".csc-cataloging-template",
            "targetSelector": ".csc-cataloging-tab .div-for-recordEditor" 
        } 
    },
    "selectors": {
        "relatedRecordsTab": ".csc-cataloging-tab"
    },
    "components": {
        "relatedRecordsTab": {
            "type": "cspace.relatedRecordsTab",
            "options": {
                "primary": "{pageBuilder}.options.primary",
                "related": "{pageBuilder}.options.related",
                "applier": "{pageBuilder}.applier",
                "model": "{pageBuilder}.model",
                "uispec": "{pageBuilder}.uispec",
                "listEditor": {
                    "options": {
                        "initList": "cspace.listEditor.receiveData",
                        "dataContext": {
                            "options": {
                                "recordType": "cataloging",
                                "dataSource": {
                                    "options": {
                                        "schema": "{pageBuilder}.schema" 
                                    } 
                                } 
                            } 
                        },
                        "details": {
                            "options": {
                                "selectors": {
                                    "identificationNumber": ".csc-object-identification-object-number" 
                                },
                                "strings": {
                                    "identificationNumberRequired": "Please specify an Identification Number" 
                                } 
                            } 
                        } 
                    } 
                }
            }
        }
    }
}