{
    "schema": [
        "recordlist",
        "recordtypes",
        "objectexit"
    ],
    "pageType": "objectexit",
    "recordType": "objectexit",
    "dataContext": {
        "options": {
            "recordType": "objectexit"
        }
    },
    "pageSpec": {
        "recordEditor": {
            "href": "%readonlypages/ObjectexitTemplate.html",
            "templateSelector": ".csc-objectexit-template",
            "targetSelector": ".csc-record-edit-container"
        },
        "footer": {
            "href": "components/footer.html",
            "templateSelector": ".csc-footer",
            "targetSelector": ".csc-footer-container"
        }
    },
    "selectors": {
        "recordEditor": ".csc-objectexit-template"
    },
    "components": {
        "tabs": {
            "type": "cspace.tabs"
        },
        "titleBar": {
            "type": "cspace.titleBar",
            "options": {
                "fields": [
                    "fields.exitNumber",
                    "fields.currentOwner"
                ]
            }
        },
        "recordEditor": {
            "type":  "cspace.recordEditor",
            "options": {
                "selectors": {
                    "identificationNumber": ".csc-objectexit-exitNumber"
                },
                "strings": {
                    "identificationNumberRequired": "Please specify Exit Number"
                },
                "dataContext": "{pageBuilder}.dataContext",
                "applier": "{pageBuilder}.applier",
                "uispec": "{pageBuilder}.uispec.recordEditor",
                "model": "{pageBuilder}.model"
            }
        },
        "sidebar": {
            "type": "cspace.sidebar"
        }
    }
}