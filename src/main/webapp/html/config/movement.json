{    
    "schema": [
        "recordlist",
        "movement"
    ],
    "pageType": "movement",
    "recordType": "movement",
    "dataContext": {
        "options": {
            "recordType": "movement" 
        } 
    },
    "pageSpec": {
        "titleBar": {
            "href": "movementTitleBar.html",
            "templateSelector": ".csc-movement-titleBar-template",
            "targetSelector": ".csc-movement-titleBar-container" 
        },
        "recordEditor": {
            "href": "%readonlyMovementTemplate.html",
            "templateSelector": ".csc-movement-template",
            "targetSelector": ".csc-record-edit-container" 
        },
        "footer": {
            "href": "footer.html",
            "templateSelector": ".csc-footer",
            "targetSelector": ".csc-footer-container" 
        } 
    },
    "selectors": {
        "titleBar": ".csc-movement-template",
        "recordEditor": ".csc-movement-template"
    },
    "components": {
        "tabs": {
            "type": "cspace.tabs"
        },
        "titleBar": {
            "type": "cspace.titleBar"
        },
        "recordEditor": {
            "type":  "cspace.recordEditor",
            "options": {
                "selectors": {
                    "identificationNumber": ".csc-movement-currentLocation" 
                },
                "strings": {
                    "identificationNumberRequired": "Please specify Current Location" 
                },
                "dataContext": "{pageBuilder}.dataContext",
                "applier": "{pageBuilder}.applier",
                "uispec": "{pageBuilder}.uispec.recordEditor",
                "model": "{pageBuilder}.model" 
            }
        },
        "sidebar": {
            "type": "cspace.sidebar",
            "options": {
                "uispec": "{pageBuilder}.uispec.sidebar",
                "applier": "{pageBuilder}.applier",
                "model": "{pageBuilder}.model",
                "primaryRecordType": "{pageBuilder}.options.pageType"
            }
        }
    }
}