{
    "pageBuilder": {
        "options": {
            "pageType": "movement",
            "dataContext": {
                "options": {
                    "recordType": "movement" 
                } 
            },
            "pageSpec": {
                "header": {
                    "href": "header.html",
                    "templateSelector": ".csc-header-template",
                    "targetSelector": ".csc-header-container" 
                },
                "tabs": {
                    "href": "tabsTemplate.html",
                    "templateSelector": ".csc-tabs-template",
                    "targetSelector": ".csc-tabs-container" 
                },
                "titleBar": {
                    "href": "movementTitleBar.html",
                    "templateSelector": ".csc-movement-titleBar-template",
                    "targetSelector": ".csc-movement-titleBar-container" 
                },
                "dateEntry": {
                    "href": "MovementTemplate.html",
                    "templateSelector": ".csc-movement-template",
                    "targetSelector": ".csc-record-edit-container" 
                },
                "sidebar": {
                    "href": "right-sidebar.html",
                    "templateSelector": ".csc-right-sidebar",
                    "targetSelector": ".csc-sidebar-container" 
                },
                "footer": {
                    "href": "footer.html",
                    "templateSelector": ".csc-footer",
                    "targetSelector": ".csc-footer-container" 
                } 
            } 
        } 
    },
    "dependencies": {
        "titleBar": {
            "funcName": "cspace.titleBar",
            "args": [
                ".csc-movement-template",
                "{pageBuilder}.applier" 
            ] 
        },
        "tabs": {
            "funcName": "cspace.tabs",
            "args": [
                ".csc-tabs-template",
                "{pageBuilder}.applier" 
            ] 
        },
        "recordEditor": {
            "funcName": "cspace.recordEditor",
            "args": [
                ".csc-movement-template",
                "{pageBuilder}.dataContext",
                "{pageBuilder}.applier",
                "{pageBuilder}.uispec.recordEditor" 
            ] 
        },
        "sidebar": {
            "funcName": "cspace.sidebar",
            "args": [
                ".csc-sidebar",
                "{pageBuilder}.applier" 
            ] 
        } 
    },
    "depOpts": {
        "titleBar": {
            "options": {
                "uispec": "{pageBuilder}.uispec.titleBar" 
            } 
        },
        "tabs": {
            "options": {
                "tabList": [
                    {"name": "Location &amp; Movement", "target": "#primaryTab"},
                    {"name": "Acquisition", "target": null},
                    {"name": "Cataloging", "target": "objectTabPlaceholder.html"},
                    {"name": "Intake", "target": null},
                    {"name": "Loan In", "target": null},
                    {"name": "Loan Out", "target": null},
                    {"name": "Location &amp; Movement - related", "target": "movementTab.html"},
                    {"name": "Media", "target": null}
                ],
                "tabSetups": [
                    "null",
                    {
                        "func": "cspace.tabSetup",
                        "options": {
                            "primaryRecordType": "{pageBuilder}.options.pageType",
                            "configURL": "./config/object-tab.json"
                        } 
                    },
                    {
                        "func": "cspace.tabSetup",
                        "options": {
                            "primaryRecordType": "{pageBuilder}.options.pageType",
                            "configURL": "./config/movement-tab.json"
                        } 
                    }
                ] 
            } 
        },
        "recordEditor": {
            "options": {
                "selectors": {
                    "identificationNumber": ".csc-movement-currentLocation" 
                },
                "strings": {
                    "identificationNumberRequired": "Please specify Current Location" 
                } 
            } 
        },
        "sidebar": {
            "options": {
                "uispec": "{pageBuilder}.uispec.sidebar",
                "primaryRecordType": "{pageBuilder}.options.pageType" 
            } 
        } 
    } 
}