{
    "pageBuilder": {
        "options": {
            "pageType": "acquisition",
            "recordType": "acquisition",
            "dataContext": {
                "options": {
                    "recordType": "acquisition" 
                } 
            },
            "pageSpec": {
                "tabs": {
                    "href": "tabsTemplate.html",
                    "templateSelector": ".csc-tabs-template",
                    "targetSelector": ".csc-tabs-container" 
                },
                "titleBar": {
                    "href": "acquisitionTitleBar.html",
                    "templateSelector": ".csc-acquisition-titleBar-template",
                    "targetSelector": ".csc-acquisition-titleBar-container" 
                },
                "dateEntry": {
                    "href": "acquisitionTemplate.html",
                    "templateSelector": ".csc-acquisition-template",
                    "targetSelector": ".csc-record-edit-container" 
                },
                "sidebar": {
                    "href": "right-sidebar.html",
                    "templateSelector": ".csc-right-sidebar",
                    "targetSelector": ".csc-sidebar-container" 
                },
                "footer": {
                    "href": "footer.html",
                    "templateSelector": ".csc-footer",
                    "targetSelector": ".csc-footer-container" 
                } 
            } 
        } 
    },
    "dependencies": {
        "titleBar": {
            "funcName": "cspace.titleBar",
            "args": [
                ".csc-acquisition-titleBar-template" 
            ] 
        },
        "tabs": {
            "funcName": "cspace.tabs",
            "args": [
                ".csc-tabs-template" 
            ] 
        },
        "recordEditor": {
            "funcName": "cspace.recordEditor",
            "args": [
                ".csc-acquisition-template" 
            ] 
        },
        "sidebar": {
            "funcName": "cspace.sidebar",
            "args": [
                ".csc-sidebar" 
            ] 
        } 
    },
    "depOpts": {
        "titleBar": {
            "options": {
                "uispec": "{pageBuilder}.uispec.titleBar",
                "applier": "{pageBuilder}.applier",
                "model": "{pageBuilder}.model" 
            } 
        },
        "tabs": {
            "options": {
                "tabList": [
                    {
                        "name": "Acquisition",
                        "target": "#primaryTab"
                    },
                    {
                        "name": "Acquisition - related",
                        "target": null
                    },
                    {
                        "name": "Cataloging",
                        "target": "objectTabPlaceholder.html"
                    },
                    {
                        "name": "Intake",
                        "target": null
                    },
                    {
                        "name": "Loan In",
                        "target": null
                    },
                    {
                        "name": "Loan Out",
                        "target": null
                    },
                    {
                        "name": "Location &amp; Movement",
                        "target": "movementTab.html"
                    },
                    {
                        "name": "Media",
                        "target": null
                    } 
                ],
                "tabSetups": [
                    "null",
                    {
                        "func": "cspace.tabSetup",
                        "options": {
                            "primaryRecordType": "{pageBuilder}.options.pageType",
                            "applier": "{pageBuilder}.applier",
                            "model": "{pageBuilder}.model",
                            "configURL": "./config/cataloging-tab.json" 
                        } 
                    },
                    {
                        "func": "cspace.tabSetup",
                        "options": {
                            "primaryRecordType": "{pageBuilder}.options.pageType",
                            "applier": "{pageBuilder}.applier",
                            "model": "{pageBuilder}.model",
                            "configURL": "./config/movement-tab.json" 
                        } 
                    } 
                ] 
            } 
        },
        "recordEditor": {
            "options": {
                "selectors": {
                    "identificationNumber": ".csc-acquisition-numberPatternChooser-reference-number" 
                },
                "strings": {
                    "identificationNumberRequired": "Please specify an Acquisition Reference Number" 
                },
                "dataContext": "{pageBuilder}.dataContext",
                "applier": "{pageBuilder}.applier",
                "uispec": "{pageBuilder}.uispec.recordEditor",
                "model": "{pageBuilder}.model" 
            } 
        },
        "sidebar": {
            "options": {
                "uispec": "{pageBuilder}.uispec.sidebar",
                "applier": "{pageBuilder}.applier",
                "model": "{pageBuilder}.model",
                "primaryRecordType": "{pageBuilder}.options.pageType" 
            } 
        } 
    } 
}