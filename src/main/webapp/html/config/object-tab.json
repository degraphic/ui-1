{
    "pageBuilder": {
        "options": {
            "pageType": "object-tab",
            "pageSpec": {
                "list": {
                    "href": "objectTabRecordListTemplate.html",
                    "templateSelector": ".csc-object-tab-record-list",
                    "targetSelector": ".csc-object-tab .div-for-list-of-records"
                },
                "details": {
                    "href": "ObjectEntryTemplate.html",
                    "templateSelector": ".csc-object-entry-template",
                    "targetSelector": ".csc-object-tab .div-for-recordEditor"
                } 
            }
        } 
    },
    "dependencies": {
        "relatedRecordsTab": {
            "funcName": "cspace.relatedRecordsTab",
            "args": [
                ".csc-object-tab",
                "{pageBuilder}.pageType",
                "objects",
                "{pageBuilder}.uispec",
                "{pageBuilder}.applier"
            ] 
        } 
    },
    "depOpts": {
        "relatedRecordsTab": {
            "options": {
                "listEditor": {
                    "options": {
                        "initList": "cspace.listEditor.receiveData",
                        "dataContext": {
                            "options": {
                                "recordType": "objects",
                                "dataSource": {
                                    "options": {
                                        "uispec": "{pageBuilder}.uispec.details" 
                                    }
                                }
                            }
                        },
                        "details": {
                            "options": {
                                "selectors": {
                                    "identificationNumber": ".csc-object-identification-object-number" 
                                },
                                "strings": {
                                    "identificationNumberRequired": "Please specify an Identification Number" 
                                } 
                            } 
                        }
                    }
                },
                "relationManager": {
                    "options": {
                        "primaryRecordType": "{pageBuilder}.options.primaryRecordType"
                    }
                }
            }
        }
    } 
}