{
    "userList": {
        ".csc-user-userList-row:": {
            "children": [
                {
                    ".csc-user-userList-name": "${items.0.name}",
                    ".csc-user-userList-status": "${items.0.status}",
                    ".csc-user-userList-csid": "${items.0.csid}"
                }
            ]
        }
    },
    "userDetails": {
        ".csc-user-userID": "${fields.userID}",
        ".csc-user-userName": "${fields.userName}", //NameAuthority
        ".csc-user-password": "${fields.password}",
        ".csc-user-passwordConfirm": "${fields.passwordConfirm}",
        ".csc-user-email": "${fields.email}",
        ".csc-user-status": "${fields.status}", // contolled list: "Active", "Inactive"
    }
}