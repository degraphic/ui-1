            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.fieldCollectionMethods",
                        "protoTree": {
                            ".csc-intake-fieldCollectionMethod": {
                                "default": "0",
                                "optionnames": [
                                    "Borehole",
                                    "Commissioned",
                                    "Exchanged",
                                    "Found dead",
                                    "Metal detector",
                                    "Netted",
                                    "Photographed",
                                    "Purchased",
                                    "Salvaged",
                                    "Shot",
                                    "Trapped"
                                ],
                                "optionlist": [
                                    "borehole",
                                    "commissioned",
                                    "exchanged",
                                    "found-dead",
                                    "metal-detector",
                                    "netted",
                                    "photographed",
                                    "purchased",
                                    "salvaged",
                                    "shot",
                                    "trapped"
                                ],
                                "selection": "${fields.fieldCollectionMethods.0.fieldCollectionMethod}" 
                            } 
                        } 
                    } 
                }
            ] 
        },
        ".csc-intake-field-collection-event-name": "${fields.fieldCollectionEventName}",
        ".csc-intake-fieldCollectionPlace":  "${fields.fieldCollectionPlace}",
        ".csc-intake-location-date": "${fields.locationDate}",
        ".csc-intake-conditionCheckMethod": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
