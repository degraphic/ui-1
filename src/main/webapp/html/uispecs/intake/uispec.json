{
    "sidebar": {
        "relatedCataloging": {
            ".csc-recordList-row:": {
                "children": [
                    {
                        ".csc-related-summary": "${items.0.summary}",
                        ".csc-related-number": {
                            "linktext": "${items.0.number}",
                            "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                        }
                    }
                ]
            }
        },
        "relatedProcedures": {
            ".csc-recordList-row:": {
                "children": [
                    {
                        ".csc-related-summary": "${items.0.summary}",
                        ".csc-related-number": {
                            "linktext": "${items.0.number}",
                            "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                        },
                        ".csc-related-recordtype": "${items.0.recordtype}"
                    }
                ]
            }
        },
        "termsUsed": {
            ".csc-recordList-row:": {
                "children": [
                    {
                        ".csc-related-number": {
                            "linktext": "${items.0.number}",
                            "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                        },
                        ".csc-related-field": "${items.0.sourceFieldName}",
                        ".csc-related-recordtype": "${items.0.recordtype}"
                    }
                ]
            }
        }
    },
    "titleBar": {
        ".csc-titleBar-intake-entry-number": "${fields.entryNumber}",
        ".csc-intake-current-owner-titlebar": "${fields.currentOwner}"
    },
    "recordEditor": {
        ".csc-intake-condition-check-date": "${fields.conditionCheckDate}",
        ".csc-intake-current-owner": {
            "valuebinding": "${fields.currentOwner}",
            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/intake/autocomplete/currentOwner",
                        "vocabUrl": "../../chain/intake/source-vocab/currentOwner"
                    }
                }
            ]
        },
        ".csc-intake-depositor": {
            "valuebinding": "${fields.depositor}",
            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/intake/autocomplete/depositor",
                        "vocabUrl": "../../chain/intake/source-vocab/depositor"
                    }
                }
            ]
        },
        ".csc-intake-conditionCheckerOrAssessor": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.conditionCheckersOrAssessors",
                        "protoTree": {
                            ".csc-intake-conditionCheckerOrAssessor": {
                                "valuebinding": "${fields.conditionCheckersOrAssessors.0.conditionCheckerOrAssessor}",
                                "decorators": [
                                    {
                                        "func": "cspace.autocomplete",
                                        "type": "fluid",
                                        "options": {
                                            "queryUrl": "../../chain/intake/autocomplete/conditionCheckerOrAssessor",
                                            "vocabUrl": "../../chain/intake/source-vocab/conditionCheckerOrAssessor"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            ]
        },
        ".csc-intake-condition-check-note": "${fields.conditionCheckNote}",
        ".csc-intake-entry-date-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-intake-currentLocationGroup": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.currentLocationGroup",
                        "protoTree": {
                            ".csc-intake-current-location": "${fields.currentLocationGroup.0.currentLocation}",
                            ".csc-intake-current-location-fitness": {
                                "optionnames": [
                                    "Please select a value",
                                    "Suitable",
                                    "Dangerous",
                                    "Unsuitable",
                                    "Reasonable"
                                ],
                                "optionlist": [
                                    "",
                                    "suitable",
                                    "dangerous",
                                    "unsuitable",
                                    "reasonable"
                                ],
                                "selection": "${fields.currentLocationGroup.0.currentLocationFitness}"
                            },
                            ".csc-intake-current-location-note": "${fields.currentLocationGroup.0.currentLocationNote}"
                        }
                    }
                }
            ]
        },
        ".csc-intake-fieldCollectionNumber": "${fields.fieldCollectionNumber}",
        ".csc-intake-insurance-policy-number": "${fields.insurancePolicyNumber}",
        ".csc-intake-fieldCollectionDate": "${fields.fieldCollectionDate}",
        ".csc-intake-conditionCheckMethod": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.conditionCheckMethods",
                        "protoTree": {
                            ".csc-intake-conditionCheckMethod": {
                                "optionnames": [
                                    "Please select a value",
                                    "Observed",
                                    "X-Rayed"
                                ],
                                "optionlist": [
                                    "",
                                    "observed",
                                    "xrayed"
                                ],
                                "selection": "${fields.conditionCheckMethods.0.conditionCheckMethod}"
                            }
                        }
                    }
                }
            ]
        },
        ".csc-intake-packing-note": "${fields.packingNote}",
        ".csc-intake-conditionCheckReason": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.conditionCheckReasons",
                        "protoTree": {
                            ".csc-intake-conditionCheckReason": {
                                "optionnames": [
                                    "Please select a value",
                                    "Conservation",
                                    "Damaged in transit",
                                    "Loan in",
                                    "Consideration",
                                    "Enquiry"
                                ],
                                "optionlist": [
                                    "",
                                    "conservation",
                                    "damagedintransit",
                                    "loanin",
                                    "consideration",
                                    "enquiry"
                                ],
                                "selection": "${fields.conditionCheckReasons.0.conditionCheckReason}"
                            }
                        }
                    }
                }
            ]
        },
        ".csc-intake-insurance-note": "${fields.insuranceNote}",
        ".csc-intake-entry-reason": {
            "default": "0",
            "optionnames": [
                "Please select a value",
                "Enquiry",
                "Consideration",
                "Commission",
                "Loan"
            ],
            "optionlist": [
                "",
                "enquiry",
                "consideration",
                "commission",
                "loan"
            ],
            "selection": "${fields.entryReason}"
        },
        ".csc-intake-location-date-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-intake-field-collection-event-name": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.fieldCollectionEventNames",
                        "protoTree": {
                            ".csc-intake-field-collection-event-name": "${fields.fieldCollectionEventNames.0.fieldCollectionEventName}"
                        }
                    }
                }
            ]
        },
        ".csc-intake-fieldCollectionNote": "${fields.fieldCollectionNote}",
        ".csc-intake-insurance-renewal-date-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-intake-insurance-reference-number": "${fields.insuranceReferenceNumber}",
        ".csc-intake-returnDate": "${fields.returnDate}",
        ".csc-intake-entry-number-container": {
            "decorators": [
                {
                    "func": "cspace.numberPatternChooser",
                    "type": "fluid",
                    "options": {
                        "model": {
                            "names": [
                                "Intake",
                                "Study",
                                "Evaluation"
                            ],
                            "list": [
                                "intake",
                                "study",
                                "evaluations"
                            ],
                            "samples": [
                                "IN2009.1",
                                "ST2009.1",
                                "EV2009.1"
                            ]
                        },
                        "selectors": {
                            "numberField": ".csc-intake-entry-number"
                        }
                    }
                }
            ]
        },
        ".csc-intake-valuer": {
            "valuebinding": "${fields.valuer}",
            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/intake/autocomplete/valuer",
                        "vocabUrl": "../../chain/intake/source-vocab/valuer"
                    }
                }
            ]
        },
        ".csc-intake-entry-date": "${fields.entryDate}",
        ".csc-intake-fieldCollectionDate-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-intake-insurance-renewal-date": "${fields.insuranceRenewalDate}",
        ".csc-intake-depositor-requirements": "${fields.depositorsRequirements}",
        ".csc-intake-entry-number": "${fields.entryNumber}",
        ".csc-intake-entryMethod": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.entryMethods",
                        "protoTree": {
                            ".csc-intake-entryMethod": {
                                "optionnames": [
                                    "Please select a value",
                                    "Found on doorstep",
                                    "In person",
                                    "Post"
                                ],
                                "optionlist": [
                                    "",
                                    "foundondoorstep",
                                    "inperson",
                                    "post"
                                ],
                                "selection": "${fields.entryMethods.0.entryMethod}"
                            }
                        }
                    }
                }
            ]
        },
        ".csc-intake-entry-note": "${fields.entryNote}",
        ".csc-intake-fieldCollectionMethod": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.fieldCollectionMethods",
                        "protoTree": {
                            ".csc-intake-fieldCollectionMethod": {
                                "optionnames": [
                                    "Please select a value",
                                    "salvaged",
                                    "borehole",
                                    "exchanged",
                                    "purchased",
                                    "shot",
                                    "photographed",
                                    "netted",
                                    "commissioned",
                                    "found dead",
                                    "trapped",
                                    "metal detector"
                                ],
                                "optionlist": [
                                    "",
                                    "salvaged",
                                    "borehole",
                                    "exchanged",
                                    "purchased",
                                    "shot",
                                    "photographed",
                                    "netted",
                                    "commissioned",
                                    "founddead",
                                    "trapped",
                                    "metaldetector"
                                ],
                                "selection": "${fields.fieldCollectionMethods.0.fieldCollectionMethod}"
                            }
                        }
                    }
                }
            ]
        },
        ".csc-intake-fieldCollector": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.fieldCollectors",
                        "protoTree": {
                            ".csc-intake-fieldCollector": {
                                "valuebinding": "${fields.fieldCollectors.0.fieldCollector}",
                                "decorators": [
                                    {
                                        "func": "cspace.autocomplete",
                                        "type": "fluid",
                                        "options": {
                                            "queryUrl": "../../chain/intake/autocomplete/fieldCollector",
                                            "vocabUrl": "../../chain/intake/source-vocab/fieldCollector"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            ]
        },
        ".csc-intake-normal-location": "${fields.normalLocation}",
        ".csc-intake-valuation-reference-number": "${fields.valuationReferenceNumber}",
        ".csc-intake-insurer": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.insurers",
                        "protoTree": {
                            ".csc-intake-insurer": {
                                "valuebinding": "${fields.insurers.0.insurer}",
                                "decorators": [
                                    {
                                        "func": "cspace.autocomplete",
                                        "type": "fluid",
                                        "options": {
                                            "queryUrl": "../../chain/intake/autocomplete/insurer",
                                            "vocabUrl": "../../chain/intake/source-vocab/insurer"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            ]
        },
        ".csc-intake-fieldCollectionPlace": "${fields.fieldCollectionPlace}",
        ".csc-intake-condition-check-reference-number": "${fields.conditionCheckReferenceNumber}",
        ".csc-intake-returnDate-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-intake-condition-check-date-container": {
            "decorators": [
                {
                    "func": "cspace.datePicker",
                    "type": "fluid"
                }
            ]
        },
        ".csc-intake-location-date": "${fields.locationDate}",
        ".csc-intake-fieldCollectionSource": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.fieldCollectionSources",
                        "protoTree": {
                            ".csc-intake-fieldCollectionSource": {
                                "valuebinding": "${fields.fieldCollectionSources.0.fieldCollectionSource}",
                                "decorators": [
                                    {
                                        "func": "cspace.autocomplete",
                                        "type": "fluid",
                                        "options": {
                                            "queryUrl": "../../chain/intake/autocomplete/fieldCollectionSource",
                                            "vocabUrl": "../../chain/intake/source-vocab/fieldCollectionSource"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                }
            ]
        }
    }
}