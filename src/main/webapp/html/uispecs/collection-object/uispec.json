        ".csc-object-description-content-language": "${fields.descContentLanguage}",
        ".csc-object-description-content-activity": "${fields.descContentActivity}",
        ".csc-object-description-content-concept": "${fields.descContentConcept}",
        ".csc-object-description-content-position": "${fields.descContentPosition}",
