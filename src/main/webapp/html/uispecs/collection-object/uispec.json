        ".csc-object-description-inscription-description-type": "${fields.descInscriptionDescriptionType}",
        ".csc-object-description-inscription-description-method": "${fields.descInscriptionDescriptionMethod}",
        ".csc-object-description-inscription-description-interpretation": "${fields.descInscriptionDescriptionInterpretation}",
        ".csc-date-information-date-association": "${fields.descDateAssociation}",
