            "decorators": [{
                "type": "fluid",
                "func": "cspace.numberPatternChooser",
                "container": ".csc-object-identification-object-number-container",
                "options": {
                    "selectors": {
                        "numberField": ".csc-object-identification-object-number"
                    },
                    "model": {
                        "list": ["intake", "objects", "loans-in"],
                        "samples": ["IN2009.1.1", "2009.1.1", "LI2009.1.1"],
                        "names": ["Intake", "Accession", "Loans In"]
                    }
                }
            }]
        },
        ".csc-object-identification-other-number": "${fields.otherNumber}",
        ".csc-object-identification-other-number-type": "${fields.otherNumberType}",
        ".csc-object-identification-brief-description": "${fields.description}",
        ".csc-object-identification-object-name": "${fields.objectName}",
        ".csc-object-identification-object-currency": "${fields.objectCurrency}",
        ".csc-object-identification-object-level": "${fields.objectLevel}",
        ".csc-object-identification-object-system": "${fields.objectSystem}",
