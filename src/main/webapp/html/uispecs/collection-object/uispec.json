        ".csc-object-identification-number-objects": "${fields.numberOfObjects}",
        ".csc-object-identification-responsible-department": {
            "selection": "${fields.responsibleDepartment}",
            "optionlist": ["antiquities", "architecture-design", "decorative-arts", "ethnography", "herpetology", "media-performance-art", "paintings-sculpture", "paleobotany", "photographs", "prints-drawings"],
            "optionnames": ["Antiquities", "Architecture and Design", "Decorative Arts", "Ethnography", "Herpetology", "Media and Performance Art", "Paintings and Sculpture", "Paleobotany", "Photographs", "Prints and Drawings"]
        },
        ".csc-object-identification-comments": "${fields.comments}",
        ".csc-object-description-copy-number": "${fields.descCopyNumber}",
        ".csc-object-description-status": "${fields.descStatus}",
        ".csc-object-description-sex": "${fields.descSex}",
        ".csc-object-description-phase": "${fields.descPhase}",
        ".csc-object-description-form": "${fields.descForm}",
